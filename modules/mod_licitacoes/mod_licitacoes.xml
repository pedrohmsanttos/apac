<?xml version="1.0" encoding="utf-8"?>
<extension type="module" version="3.1.0" client="site" method="upgrade">
    <name>Exibição de Licitações</name>
    <author>Inhalt Soluções Em Informática</author>
    <version>1.0.0</version>
    <description>Módulo para da Listagem das licitações e download dos aquivos.</description>
    <files>
        <filename>mod_licitacoes.xml</filename>
        <filename module="mod_licitacoes">mod_licitacoes.php</filename>
        <filename>index.html</filename>
        <filename>helper.php</filename>
        <filename>tmpl/index.html</filename>
        <filename>tmpl/default.php</filename>
    </files>
    <config>
        <fields name="params">
            <fieldset name="basic" addfieldpath="/administrator/components/com_content/models/fields">
                <field
                    name="catid"
                   type="category"
                   extension="com_content"
                   label="Categoria:"
                   multiple="true"
                   description=""
                />
            </fieldset>

        </fields>
    </config>
</extension>
