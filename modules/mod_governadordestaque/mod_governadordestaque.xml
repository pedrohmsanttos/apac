<?xml version="1.0" encoding="utf-8"?>
<extension type="module" version="3.1.0" client="site" method="upgrade">
    <name>Exibição de Governador Atual</name>
    <author>Inhalt Soluções Em Informática</author>
    <version>1.0.0</version>
    <description>Módulo para exibição de aceontece.</description>
    <files>
        <filename>mod_governadordestaque.xml</filename>
        <filename module="mod_governadordestaque">mod_governadordestaque.php</filename>
        <filename>index.html</filename>
        <filename>tmpl/index.html</filename>
        <filename>tmpl/default.php</filename>
    </files>
    <config>
    <fields name="params">
        <fieldset name="basic">
            <field name="nome" type="text" label="Nome" filter="string" />
            <field name="catid" type="category" extension="com_agenda" label="Categoria da agenda" description="" />
            <field name="linkredesocias" type="text" label="Link p/ redes sociais" default="http://" filter="string" />
            <field name="conteudo" type="editor" label="Conteúdo" description="" width="300" filter="safehtml" />
            <field name="dados" type="editor" label="Dados" description="" width="300" filter="safehtml" />
        </fieldset>
    </fields>
</config>
</extension>
